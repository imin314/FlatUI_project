//- registration.pug
extends ../../pug/layout.pug

block title
  title Register

block content
  .registration-page.js-registration-page
    main.registration-page__main
      header.registration-page__header
        h1.registration-page__title Join us at Web Talks 2019
        .registration-page__bread-crumbs
          +breadCrumbs({ active: 1, max: 3 })
      .registration-page__form
        +registrationForm()
    footer.registration-page__footer
      +footerSection()
  