clearfix()
  &:before
  &:after
    content: ""
    display: table
  &:after
    clear: both
  zoom: 1 if support-for-ie

remify(value) {
    u = unit(value);
    template-font-size = 18.2px
    if (u is 'px') {
        return unit(value/template-font-size, 'rem');
    } else {
        return unit(value, u);
    }
}